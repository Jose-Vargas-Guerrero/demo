    .contact{
		height: 250px;
		max-width: 100vw;

		display: flex;
		flex-direction: column;
		justify-content: center;
		align-items: center;
	}

	.contact > h2{
		font-size: 2.3rem;
		font-weight: bolder;
	}

	.contact > a{
		color: #7f7f7f;
		opacity: 0.9;
		font-size: 2.2rem;
		text-decoration: none;
		font-weight: bolder;
		margin-top: 10px;
		transition: opacity 0.3s ease-out;
	}

	.contact > a:hover {
		opacity: 0.5;
	}

